<h1>My tasks</h1>
<% @tasks.each do |task| %>
  <p>
    <ul>
      <li>
      <label class= "contaier">
      <input type="checkbox" <&="checked" if task.completed?%>><%= link_to task.title, task_path(task.id) %></li>
    </ul>
  </p>
<% end %>

<div>
<%= link_to "+ Add a new task", tasks_new_path%>
</div>
<%#= render "form", task: @task %>
